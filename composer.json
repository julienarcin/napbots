{
    "name": "julienarcin/napbots",
    "description": "Napbots control center.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/julienarcin/napbots/issues",
        "source": "https://github.com/julienarcin/napbots"
    },
    "authors": [
        {
            "name": "Julien Arcin",
            "email": "julien.arcin@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/log": "^8.0",
        "laravel-zero/framework": "^8.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["napbots"]
}
